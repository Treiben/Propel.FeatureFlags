{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "PropelFeatureFlags": {
        "SqlConnectionString": "Host=postgres;Port=5432;Database=propel;Username=propel_user;Password=propel_password;Search Path=usage_demo;Include Error Detail=true",
        "RedisConnectionString": "redis:6379",
        "DefaultTimeZone": "UTC",
        "cacheOptions": {
            "ExpiryInMinutes": 5,
            "SlidingExpiryInMinutes": 30,
            "UseCache": true
        }
    }
}
