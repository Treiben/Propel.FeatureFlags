{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "PropelFeatureFlags": {
        "database": {
            "DefaultConnection": "Host=postgres;Port=5432;Database=propel;Username=propel_user;Password=propel_password;Search Path=usage_demo;Include Error Detail=true"
        },
        "cache": {
            "UseCache": true,
            "CacheDurationInMinutes": 30,
            "SlidingDurationInMinutes": 15,
            "Connection": "redis:6379"
        },
        "DefaultTimeZone": "UTC"
    }
}
